{
  "$schema": "http://json-schema.org/schema#",
  "id": "http://yourdomain.com/schemas/gdbots/gdbots/pbj/mixin/entity/1-0-0.json",
  "type": "object",
  "properties": {
    "_id": {
      "type": "string",
      "required": true,
      "additionalProperties": {
        "customType": "identifier",
        "rule": "single",
        "overridable": true
      }
    },
    "etag": {
      "type": "string",
      "pattern": "^[A-Za-z0-9_\\-]+$",
      "additionalProperties": {
        "customType": "string",
        "rule": "single"
      }
    },
    "created_at": {
      "type": "number",
      "additionalProperties": {
        "customType": "microtime",
        "rule": "single"
      }
    },
    "updated_at": {
      "type": "number",
      "additionalProperties": {
        "customType": "microtime",
        "rule": "single"
      }
    }
  },
  "required": [
    "_id"
  ],
  "additionalProperties": false
}