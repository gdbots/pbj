{
  "name": "pbjc/example",
  "type": "library",
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev",
    "gdbots/pbj": "dev-aws3 as dev-master",
    "gdbots/pbjc": "dev-master",
    "gdbots/schemas": "dev-xml"
  },
  "autoload": {
    "files": ["pbj-schemas.php"],
    "psr-0": {
      "": "src"
    }
  },
  "autoload-dev": {
    "files": ["pbj-schema-stores.php"],
    "psr-0": {
      "": "tests"
    }
  },
  "scripts": {
    "post-package-install": [
      "Gdbots\\Pbjc\\Composer\\ScriptHandler::writePbjSchemaStoresFile"
    ],
    "post-package-uninstall": [
      "Gdbots\\Pbjc\\Composer\\ScriptHandler::writePbjSchemaStoresFile"
    ],
    "post-package-update": [
      "Gdbots\\Pbjc\\Composer\\ScriptHandler::writePbjSchemaStoresFile"
    ]
  }
}
