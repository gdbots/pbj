<schema-mapping xmlns="gdbots:pbjc:schema-mapping"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <entity id="pbj:gdbots:pbj:mixin:command:1-0-1" mixin="true">
