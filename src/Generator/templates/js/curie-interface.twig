{% set className = getClassName(schema) %}
'use strict';

{% block use_statements %}
import SystemUtils from 'gdbots/common/util/system-utils';
{% if schema.extends and not isSameNamespace(schema, schema.extends) %}
{% if not hasOtherMajorRev(schema) %}
import {{ getClassName(schema.extends, true) }} from '{{ schema.extends.language('js').get('namespace') }}/{{ schema.extends.id.message }}-v{{ schema.extends.id.version.major }}';
{% else %}
import {{ getClassName(schema.extends, false, className) }} from '{{ schema.extends.language('js').get('namespace') }}/{{ schema.extends.id.message }}';
{% endif %}
{% else %}
import Message from 'gdbots/pbj/message';
{% endif %}
{% endblock use_statements %}

{% block class_definition %}
export default class {{ className }} extends SystemUtils.mixinClass({% if schema.extends %}{% if not hasOtherMajorRev(schema) %}{{ getClassName(schema.extends, true) }}{% else %}{{ getClassName(schema.extends, false, className) }}{% endif %}{% else %}Message{% endif %})
{% endblock class_definition %}
{
{% block class_body %}
{% endblock class_body %}
}
