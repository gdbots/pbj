<?php

/*
 * Registers all of the pbj schemas with the MessageResolver.
 * File will be generated by the compiler (wip).
 */
\Gdbots\Pbj\MessageResolver::registerMap([
{% for schema in schemas %}
{% if not schema.isMixinSchema and schema.id.namespace in namespaces %}
    '{{ schema.id.curie }}' => '{{ schema.languageKey('php', 'namespace') }}\{{ getClassName(schema, true) }}',
{% endif %}
{% endfor %}
]);
