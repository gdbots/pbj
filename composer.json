{
  "name": "gdbots/pbjc",
  "homepage": "https://github.com/gdbots/pbjc-php",
  "description": "Compiler for converting pbj schemas into jsonschema, php, js, etc.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.6",
    "gdbots/common": "~0.1|~1.0",
    "symfony/filesystem": "~3.0",
    "symfony/finder": "~3.0",
    "twig/twig": "~1.2"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev",
    "phpunit/phpunit": "~4.3"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Pbjc\\": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Gdbots\\Tests\\Pbjc\\": "tests"
    }
  },
  "bin": ["bin/pbjc"],
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
