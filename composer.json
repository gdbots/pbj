{
  "name": "gdbots/pbjc",
  "homepage": "https://github.com/gdbots/pbjc-php",
  "description": "Compiler for converting pbj schemas into jsonschema, php, js, etc.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/pbj": "~1.1|~2.0",
    "symfony/filesystem": "~3.3|~4.0",
    "symfony/finder": "~3.3|~4.0",
    "twig/twig": "~1.35"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev",
    "phpunit/phpunit": "~4.3",
    "justinrainbow/json-schema": "~1.6"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Pbjc\\": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Gdbots\\Tests\\Pbjc\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "bin": [
    "bin/pbjc"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
